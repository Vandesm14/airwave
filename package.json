{
  "name": "airwave",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "pnpm prettier --write .",
    "format:check": "pnpm prettier --check .",
    "client-web:dev": "pnpm --dir client-web dev",
    "client-web:build": "pnpm --dir client-web build",
    "client-web:preview": "pnpm --dir client-web preview",
    "run-all": "zellij --layout zellij.kdl",
    "run-all:record": "zellij --layout zellij-record.kdl",
    "run-all:debug": "zellij --layout zellij-debug.kdl"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "prettier": "^3.0.0"
  }
}
